{
  "expo": {
    "name": "Merke und Male",
    "slug": "merke-und-male",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon.png",
    "userInterfaceStyle": "light",
    "scheme": "merkeundmale",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#60D5FA"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.s540d.merkeundmale"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#60D5FA"
      },
      "package": "com.s540d.merkeundmale",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/icons/favicon.png",
      "bundler": "metro",
      "output": "static",
      "baseUrl": "/DrawFromMemory/"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/your-project-id-here"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
